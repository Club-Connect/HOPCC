{
  "name": "hopkins-club-connect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "yarn prisma:dev && next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write \"{prisma,src}/**/*.{ts,tsx}\"",
    "docker:up": "docker compose up --detach --no-recreate --wait",
    "docker:down": "docker compose down --timeout 5",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:seed": "NODE_ENV=development prisma db seed",
    "prisma:dev": "yarn docker:up && dotenv -e .env -- prisma migrate dev",
    "prisma:dev:studio": "yarn docker:up && dotenv -e .env -- prisma studio",
    "prisma:dev:reset": "yarn docker:up && dotenv -e .env -- prisma migrate reset --force"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.21.3",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@mantine/core": "^6.0.13",
    "@mantine/dates": "^6.0.13",
    "@mantine/dropzone": "^6.0.13",
    "@mantine/form": "^6.0.13",
    "@mantine/hooks": "^6.0.13",
    "@mantine/modals": "^6.0.13",
    "@mantine/next": "^6.0.13",
    "@mantine/notifications": "^6.0.13",
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.3.1",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/client": "^10.26.0",
    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "dayjs": "^1.11.8",
    "dotenv-cli": "^7.2.1",
    "next": "^13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.12.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.16.0",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "prisma": "^4.14.0",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.13.2"
  }
}
